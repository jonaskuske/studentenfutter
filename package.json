{
  "name": "studentenfutter",
  "version": "1.0.0",
  "description": "ðŸ¥— Web app for student cookbook \"studentenfutter\"",
  "main": "index.js",
  "scripts": {
    "start": "run-p serve css:watch",
    "build": "npm run css:build",
    "serve": "php -S localhost:8080 -t src src/kirby/router.php",
    "css:watch": "postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.dev.css -w",
    "css:build": "cross-env NODE_ENV=production postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonaskuske/studentenfutter.git"
  },
  "author": "Jonas Kuske",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/jonaskuske/studentenfutter/issues"
  },
  "homepage": "https://github.com/jonaskuske/studentenfutter#readme",
  "devDependencies": {
    "@prettier/plugin-php": "^0.14.2",
    "autoprefixer": "^9.8.1",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-focus-visible": "^5.0.0",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-smoothscroll-anchor-polyfill": "^2.0.1",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.6",
    "tailwindcss-aspect-ratio": "^3.0.0"
  },
  "dependencies": {},
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "tabWidth": 2,
    "phpVersion": "7.4"
  }
